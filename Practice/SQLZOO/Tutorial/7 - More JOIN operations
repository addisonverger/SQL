--All resources provided by SQLZOO: https://new.sqlzoo.net/wiki/The_JOIN_operation

--1. 1962 movies

SELECT id, title
FROM movie
WHERE yr = 1962

--2. When was Citizen Kane released?

SELECT yr
FROM movie
WHERE title = 'Citizen Kane'

--3. Star Trek movies

SELECT id, title, yr
FROM movie
WHERE title LIKE '%Star Trek%'
ORDER BY yr
